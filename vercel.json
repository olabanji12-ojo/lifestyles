{
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/sitemaps/:path*",
      "destination": "/sitemaps/:path*"
    },
    {
      "source": "/((?!sitemap|sitemaps|api).*)",
      "destination": "/index.html"
    }
  ]
}